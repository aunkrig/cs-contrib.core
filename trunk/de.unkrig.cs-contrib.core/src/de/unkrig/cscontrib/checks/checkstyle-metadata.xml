    <!-- WrapAndIndent -->

    <rule-group-metadata name="%Whitespace.group" priority="999">
        <rule-metadata
            internal-name="de.unkrig.cscontrib.checks.WrapAndIndent"
            parent="TreeWalker"
            name="%de.unkrig.cscontrib.checks.WrapAndIndent.name"
        >
            <alternative-name internal-name="de.unkrig.cscontrib.checks.WrapAndIndent"/>
            <description>%de.unkrig.cscontrib.checks.WrapAndIndent.desc</description>
            <property-metadata name="basicOffset" datatype="Integer" default-value="4">
                <description>%de.unkrig.cscontrib.checks.WrapAndIndent.basicOffset</description>
            </property-metadata>

            <message-key key="Must wrap line before ''{0}''"/>
            <quickfix classname="de.unkrig.cscontrib.ui.quickfixes.WrapAndIndent1"/>

            <message-key key="''{0}'' must appear on same line as ''{1}''"/>
            <quickfix classname="de.unkrig.cscontrib.ui.quickfixes.WrapAndIndent2"/>

            <message-key key="''{0}'' must appear in column {1}, not {2}"/>
        </rule-metadata>
    </rule-group-metadata>

